    "animation",
    "animation-group",
    "transitions"
  ],
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/josepot/react-animation-group"
  },
  "homepage": "https://github.com/josepot/react-animation-group#readme",
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "chain-function": "^1.0.0",
    "loose-envify": "^1.3.1",
    "prop-types": "^15.5.6",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "release-script": "^1.0.2",
    "rimraf": "^2.6.1",
    "teaspoon": "^6.4.3",
    "webpack": "^2.2.1"
  },
  "release-script": {
    "altPkgRootFolder": "lib"
